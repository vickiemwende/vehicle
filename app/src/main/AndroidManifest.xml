        <activity
            android:name=".InterimActivity"
            android:exported="false" />
        <activity
            android:name=".CoolingActivity"
            android:exported="false" />
        <activity
            android:name=".EngineRepairActivity"
            android:exported="false" />
        <activity
            android:name=".OilChangeActivity"
            android:exported="false" />
        <activity
            android:name=".PaintingActivity"
            android:exported="false" />
        <activity
            android:name=".PunctureActivity"
            android:exported="false" />
        <activity
            android:name=".SteeringActivity"
            android:exported="false" />
    </application>

</manifest>